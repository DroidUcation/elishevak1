<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.perfecto.optimizer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- <activity android:name="com.perfecto.optimizer.activities.MainActivity"></activity> -->
        <activity android:name=".preferences.customizedPreferences.CustomizedPreferencesActivity" />
        <activity android:name=".preferences.DeviceTypesActivity" />
        <activity android:name=".preferences.OperatingSystemsActivity" />
        <activity android:name=".preferences.targetAudience.TargetAudiencesActivity" />
        <activity android:name=".preferences.targetAudience.GeographiesActivity" />
        <activity android:name=".preferences.customizedPreferences.CustomersDevicesActivity" />
        <activity android:name=".ResultsActivity"></activity>
    </application>

</manifest>